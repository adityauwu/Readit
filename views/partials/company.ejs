<h4 class="text-center">Company (<%=company.length %>)</h4>
<table class="table table-striped">
    <tbody>
<% if (company != undefined) {
    company.forEach((s) => { %>
        <tr>
        <td><a href="/r/<%= s.name %>">/r/<%= s.name %></a></td>
        </tr>
    <% }); %>
    <% if(company.length == 0) { %>
        You are not subscribed to any company
    <% } %>
<% } %>
    </tbody>
</table>
